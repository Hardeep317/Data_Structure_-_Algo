nums1 = [4,1,2], nums2 = [1,3,4,2]

int [] res = new int [nums1];
int [] arr = new int [10001];

for(int i = 0; i < nums2; i++){
    int j = i;
    while( j < nums2.length && nums2[i] < nums2[i]){
        i++;
    }
    if(j >= nums2.length){
        arr[nums2[i]] = -1;
    }else{
        arr[nums2[i]] = nums2[j];
    }
}

for(int i = 0; i < nums1.length; i++){
    res[i] = arr[nums1[i]];
}

return res;